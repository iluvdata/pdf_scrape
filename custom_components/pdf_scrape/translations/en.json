  "config_subentries": {
    "target": {
      "initiate_flow": {
        "user": "Add Search Target"
      },
      "entry_type": "Scrape Target",
      "step": {
        "user": {
          "title": "Target Page",
          "description": "{title}",
          "data": {
            "pdf_pages": "Pages"
          },
          "data_description": {
            "pdf_pages": "Pages in PDF: {pages}. Use comma to separate multiple pages or hyphen for page ranges (e.g., 1,3-5,7)."
